{
  "name": "sanctuary-scripts",
  "version": "6.0.0",
  "description": "Shell scripts used in multiple Sanctuary projects",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/sanctuary-js/sanctuary-scripts.git"
  },
  "type": "module",
  "files": [
    "/bin/",
    "/LICENSE",
    "/README.md",
    "/functions",
    "/package.json"
  ],
  "bin": {
    "sanctuary-check-required-files": "bin/check-required-files",
    "sanctuary-doctest": "bin/doctest",
    "sanctuary-generate-readme": "bin/generate-readme",
    "sanctuary-lint": "bin/lint",
    "sanctuary-lint-commit-messages": "bin/lint-commit-messages",
    "sanctuary-lint-json": "bin/lint-json",
    "sanctuary-lint-package-json": "bin/lint-package-json",
    "sanctuary-prepublish": "bin/prepublish",
    "sanctuary-release": "bin/release",
    "sanctuary-test": "bin/test",
    "sanctuary-update-copyright-year": "bin/update-copyright-year"
  },
  "engines": {
    "node": ">=12.22.0",
    "npm": ">=3.0.0"
  },
  "dependencies": {
    "c8": "8.0.x",
    "doctest": "0.21.x",
    "eslint": "8.55.x",
    "oletus": "4.0.x",
    "sanctuary-style": "8.0.x",
    "transcribe": "1.1.2",
    "xyz": "5.0.x"
  },
  "devDependencies": {},
  "scripts": {
    "doctest": "bin/doctest",
    "lint": "bin/lint",
    "release": "bin/release",
    "test": "npm run lint && bin/test && npm run doctest"
  }
}
